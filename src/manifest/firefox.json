{
  "manifest_version": 2,
  "short_name": "Firefox extension.",
  "background": {
    "scripts": [
      "eventPage.js"
    ]
  },
  "homepage_url": "https://github.com/alanhg/chrome-extension-react-typescript-boilerplate",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "react-typescript-boilerplate",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>"
  ]
}
