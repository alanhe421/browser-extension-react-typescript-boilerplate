{
  "manifest_version": 3,
  "minimum_chrome_version": "72",
  "background": {
    "service_worker": "eventPage.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
